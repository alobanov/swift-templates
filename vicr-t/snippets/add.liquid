{% if custom_parameters.model > '' -%}
{% assign domainModel = custom_parameters.model -%}
{% else -%}
{% assign domainModel = "<#DataDomainModel#>" -%}
{% endif -%}
{% if custom_parameters.type == 'table' -%}
{% assign domainModel = "[" | append: domainModel | append:  "]" -%}
{% endif -%}
{% if custom_parameters.ui_import > '' %}
{% assign ui_imports = "import " | append: custom_parameters.ui_import -%}
{% else -%}
{% assign ui_imports = "import " | append: "UIKit" -%}
{% endif -%}
